{
    "name": "jqtree",
    "version": "1.4.12",
    "description": "Tree widget for jQuery",
    "keywords": [
        "jquery-plugin",
        "tree"
    ],
    "license": "Apache-2.0",
    "main": "./lib/tree.jquery.js",
    "types": "./src/tree.jquery.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbraak/jqtree"
    },
    "scripts": {
        "test": "karma start --browsers ChromeHeadlessNoSandbox --single-run",
        "lint": "eslint src/ src_test/ --ext .ts,.tsx",
        "production": "./production",
        "karma": "karma start",
        "jekyll-build": "bundle exec jekyll build",
        "jekyll-serve": "bundle exec jekyll serve",
        "devserver": "webpack-dev-server --mode development",
        "prettier": "prettier src/*.ts src_test/*.ts --write --tab-width 4",
        "coveralls": "coveralls < coverage/lcov.info"
    },
    "dependencies": {
        "jquery": ">=1.9"
    },
    "devDependencies": {
        "@types/jquery": "3.3.30",
        "@types/jquery-mockjax": "^2.0.31",
        "@types/qunit": "^2.9.0",
        "@typescript-eslint/eslint-plugin": "^2.6.1",
        "@typescript-eslint/parser": "^2.6.1",
        "autoprefixer": "^9.6.1",
        "coveralls": "^3.0.5",
        "eslint": "^6.6.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jquery-mockjax": "^2.5.0",
        "jsonfile": "^5.0.0",
        "karma": "^4.2.0",
        "karma-chrome-launcher": "^3.0.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "^2.0.6",
        "karma-qunit": "^4.0.0",
        "karma-webpack": "^4.0.2",
        "lodash.template": "^4.5.0",
        "postcss": "^7.0.17",
        "postcss-cli": "^6.1.3",
        "postcss-load-config": "^2.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.1.2",
        "prettier": "^1.18.2",
        "qunit": "^2.9.2",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3",
        "webpack": "^4.36.1",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.7.2"
    }
