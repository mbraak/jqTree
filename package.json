{
    "name": "jqtree",
    "version": "1.4.12",
    "description": "Tree widget for jQuery",
    "keywords": [
        "jquery-plugin",
        "tree"
    ],
    "license": "Apache-2.0",
    "main": "./lib/tree.jquery.js",
    "types": "./src/tree.jquery.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbraak/jqtree"
    },
    "scripts": {
        "test": "jest --coverage --no-cache",
        "test-watch": "jest --watch",
        "lint": "eslint src/ --ext .ts,.tsx",
        "production": "./production",
        "jekyll-build": "bundle exec jekyll build",
        "jekyll-serve": "bundle exec jekyll serve",
        "devserver": "webpack-dev-server --mode development",
        "devserver-with-coverage": "COVERAGE=true webpack-dev-server --mode development",
        "prettier": "prettier src/*.ts src_test/*.ts --write --tab-width 4",
        "coveralls": "coveralls < combined-coverage/lcov.info",
        "tsc": "tsc --noEmit --project tsconfig.json",
        "cypress-run": "start-server-and-test devserver-with-coverage http://localhost:8080 'cypress run'",
        "merge-coverage": "istanbul-combine -d combined-coverage -p detail -r lcov coverage/coverage-final.json cypress-coverage/coverage-final.json"
    },
    "dependencies": {
        "jquery": ">=1.9"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.8.1",
        "@testing-library/dom": "^7.21.7",
        "@types/jest": "^26.0.8",
        "@types/jquery": "^3.5.0",
        "@types/jquery-mockjax": "^2.0.31",
        "@types/qunit": "^2.9.1",
        "@typescript-eslint/eslint-plugin": "^3.4.0",
        "@typescript-eslint/parser": "^3.4.0",
        "autoprefixer": "^9.8.4",
        "coveralls": "^3.1.0",
        "cypress": "^4.12.1",
        "eslint": "^7.3.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "givens": "^1.3.4",
        "istanbul-combine": "^0.3.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jest": "^26.2.2",
        "jest-extended": "^0.11.5",
        "jquery-mockjax": "^2.5.0",
        "jsonfile": "^6.0.1",
        "lodash.template": "^4.5.0",
        "postcss": "^7.0.32",
        "postcss-cli": "^7.1.0",
        "postcss-load-config": "^2.1.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.2.1",
        "prettier": "^2.0.5",
        "start-server-and-test": "^1.11.3",
        "ts-jest": "^26.1.4",
        "ts-loader": "^8.0.2",
        "typescript": "^3.9.5",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "nyc": {
        "report-dir": "cypress-coverage"
    }
}
